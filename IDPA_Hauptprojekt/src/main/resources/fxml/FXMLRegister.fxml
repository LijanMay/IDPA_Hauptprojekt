<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.bbbaden.idpa_hauptprojekt.FXMLRegisterController">
   <children>
      <Button fx:id="register" layoutX="240.0" layoutY="288.0" mnemonicParsing="false" onAction="#handleRegister" prefHeight="26.0" prefWidth="120.0" text="Registrieren" />
      <Button fx:id="cancel" layoutX="240.0" layoutY="330.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="26.0" prefWidth="120.0" text="Abbrechen" />
      <Label fx:id="WIQI" layoutX="480.0" layoutY="43.0" text="WIQI" />
      <TextField fx:id="user" layoutX="140.0" layoutY="176.0" prefWidth="150.0" promptText="Benutzername" />
      <TextField fx:id="pswd" layoutX="140.0" layoutY="233.0" prefWidth="150.0" promptText="Passwort" />
      <Label layoutX="140.0" layoutY="156.0" prefWidth="125.0" text="Benutzername:" />
      <Label layoutX="140.0" layoutY="213.0" text="*Passwort:" />
      <TextField fx:id="pswd2" layoutX="310.0" layoutY="233.0" prefWidth="150.0" promptText="Bestätige Passwort" />
      <TextField fx:id="surname" layoutX="140.0" layoutY="122.0" onKeyReleased="#handleWritesname" prefWidth="150.0" promptText="Name" />
      <TextField fx:id="prename" layoutX="310.0" layoutY="122.0" onKeyReleased="#handleWritepname" prefWidth="150.0" promptText="Vorname" />
      <Label layoutX="140.0" layoutY="103.0" text="*Name:" />
      <Label layoutX="310.0" layoutY="103.0" text="*Vorname:" />
      <TextField fx:id="mail" layoutX="310.0" layoutY="176.0" prefWidth="150.0" promptText="Email" />
      <Label layoutX="310.0" layoutY="156.0" text="*Email:" />
      <Label layoutX="310.0" layoutY="213.0" text="*Passwort bestätigen:" />
      <Label layoutX="14.0" layoutY="367.0" text="* Pflichtfelder" />
   </children>
</AnchorPane>
